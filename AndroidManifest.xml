<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.bloodbank.riyaz">

    <uses-permission android:name="android.permission.CALL_PHONE" />
    <uses-permission android:name="android.permission.SEND_SMS" />



    <application
        android:name="com.bloodbank.riyaz.AppController"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.bloodbank.riyaz.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.bloodbank.riyaz.GetProvinceActivity"
            android:label="@string/app_name">
        </activity>
        <activity android:name="com.bloodbank.riyaz.GetDistrictActivity"
            android:label="@string/app_name">
        </activity>
        <activity android:name="com.bloodbank.riyaz.GetProfileActivity"
            android:label="@string/app_name">
        </activity>
    </application>

</manifest>